{
    "name": "lighth7015/simple-xml-mapper",
    "description": "Uses reflection to map XML to PHP objects.",
    "keywords": ["xml", "mapper", "object", "reflection"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gridley",
            "email": "me@robgridley.com"
        }
    ],
    "require": {
        "php": "^7.6.0 || ^8.0.0",
        "symfony/property-info": "^3.2",
        "phpdocumentor/reflection-docblock": "^5.3.0",
        "doctrine/inflector": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "SimpleXmlMapper\\": "src/"
        }
    }
}
